{
  "root": true,
  "plugins": ["prettier", "jest"],
  "extends": ["prettier", "eslint:recommended"],
  "rules": {
    "no-unused-vars": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".d.ts", ".ts", ".tsx"],
        "moduleDirectory": ["node_modules", "src"]
      }
    }
  }
}
